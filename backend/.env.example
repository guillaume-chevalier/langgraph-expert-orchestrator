# Backend Environment Variables Example
# Copy this file to .env and add your OpenAI API key

# OpenAI Configuration
OPENAI_API_KEY=sk-your-openai-api-key-here
OPENAI_MODEL=gpt-4.1
OPENAI_TEMPERATURE=0.0
OPENAI_MAX_TOKENS=32768

# Backend Server
LOG_LEVEL=INFO
BACKEND_HOST=0.0.0.0
BACKEND_PORT=8000

# Dataset Configuration
USE_MOCK_DATASETS=false
HOSTS_DATASET_PATH=dataset/hosts_dataset.json
CERTIFICATES_DATASET_PATH=dataset/web_properties_dataset.json
